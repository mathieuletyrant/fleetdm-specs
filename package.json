{
  "name": "fleetdm-openapi-generator",
  "version": "1.0.0",
  "description": "Generator for OpenAPI 3.1 specs from FleetDM REST API using Zod",
  "scripts": {
    "generate": "ts-node src/generate-openapi.ts",
    "serve": "npm run generate && swagger-ui-watcher openapi.json",
    "dev": "npm run serve"
  },
  "keywords": [
    "fleetdm",
    "openapi",
    "api",
    "typescript",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "swagger-ui-watcher": "^2.1.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
