{
  "name": "fleetdm-openapi-generator",
  "version": "1.0.0",
  "description": "Generator for OpenAPI 3.1 specs from FleetDM REST API using Zod",
  "scripts": {
    "generate": "tsx src/generate-openapi.ts",
    "serve": "tsx src/swagger-preview.ts",
    "orval": "orval --config orval.config.js"
  },
  "keywords": [
    "fleetdm",
    "openapi",
    "api",
    "typescript",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "express": "^5.2.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "orval": "^7.17.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
